    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-packagejson": "2.4.8",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
